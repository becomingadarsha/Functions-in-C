#include<stdio.h>
#include<conio.h>
int fact(); // return type with no arguments
void main()
{
	int x;
	x= fact();
	printf("The factorial of is %d",x);
    getch();
}
int fact()
{
	int a=1,i;
	printf("Enter any positive number:");
	scanf("%d",&a);
    for(i=1;i<=a;i++)
    {
    	a=a*i;
	}
	return (a);
}
