//WAP to find surface area of box using function.
#include<stdio.h>
void area(float,float,float); //argument
void main()
{
	float l,b,h,ar;
	printf("Enter value of l,b,h respectively:");
	scanf("%f%f%f",&l,&b,&h);
     area(l,b,h);

}
void area(float l,float b, float h)
{
	float x;
	x=2*(l*b+b*h+l*h);
		printf("The surface area of box is: %f",x); //but not return type
}

