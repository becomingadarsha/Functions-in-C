//11.	WAP that uses function isprime() for testing whether a number is prime or not.
#include<stdio.h>
int isprime(int); // argument and return type
void main()
{
	int n,c=0;
	printf("Enter any number:");
	scanf("%d",&n);
	isprime(n);
	 if (c==2)
    printf ("The number is PRIME");
    else
    printf ("The number is not prime");
}
int isprime(n)
{
	int i,c=0;
    for(i=1;i<=n;i++)
    {
    	if(n%i==0)
        c=c+1;
	}
}
