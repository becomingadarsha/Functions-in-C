#include<stdio.h>
void star(int,int,int,int); //argument but no return type
void main()
{
	int i,j,k,l;
	star(i,j,k,l);
	
	getch();
}
     void star(int i,int j,int k,int l)
{
	int c=0;
for(i=5;i>=1;i--)
 {
  for(j=1;j<i;j++)
  {
   printf(" ");
  }
  c=c+1;
  for(k=1;k<=c;k++)
  {
   printf("*");
  }
  for(l=1;l<=c;l++)
  {
   if (l!=1)
   printf("*");
  }
  printf("\n");
 }
}
