//WAP a program to calculate base raised to the power by using function
#include<stdio.h>
#include<math.h>
float ans(); //no argument but reurn type
void main()
{
 float x;
	x=ans();
	printf("The required answer id %f",x);
}
float ans()
{
	float x;
	int a,b;
	printf("Enter base and power of the number:");
	scanf("%d%d",&a,&b);
	x=pow(a,b);
	return (x);
}
