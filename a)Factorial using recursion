#include<stdio.h>
int fact(int);
void main()
{
	int n,result;
	printf("Enter any integer:");
	scanf("%d",&n);
	result=fact(n);
	printf("Factorial of %d is %d",n,result);
}
int fact(n)
{
	int x;
	if(n==0)
	{
		
		return 1;
	}
	else if(n==1)
	{
		return 1;
	}
	else
	{
		x=n*fact(n-1);
		return x;
	}
}
