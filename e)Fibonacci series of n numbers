#include<stdio.h>
int fibo(int n);//Fibonacci series using recursion
int main(){
	int i,num,x;
	printf("enter how many terms of Fibonacci series you want to see\n");
	scanf("%d",&num);
	for(i=1;i<=num;i++)
	{
		printf("%d\t",fibo(i));
	}
	getch();
	return 0;
}
int fibo(int n)
{
	if(n==1||n==2)
	{
		return 1;
	}
	else
	{
		return fibo(n-1)+fibo(n-2);
	}
}
